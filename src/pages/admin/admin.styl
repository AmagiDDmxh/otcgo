.container-padding {
  margin-top: 15px;
  padding: 0 32px;
  color: #747474;
}

.panel-state {
  height: 35px !important;
  border: none;
  background-color: #f2f2f2;
}

.panel-state .panel-body {
  height: 100%;
  padding-top: 6px;
  padding-bottom: 5px;
  padding-left: 27px;
  padding-right: 27px;
  background-color: #f2f2f2;
  color: #009cfd;
}

/*left-menu*/

.left-menu .nav {
  font-size: 16px;
  width: 100%;
}

.left-menu .nav .title {
  background: #009cff;
  color: #fff;
}

.left-menu .nav li a {
  border-radius: 0;
  background: #f2f2f2;
}

.admin-footer-line {
  margin-top: 20px;
  border-top: 1px solid #ccc;
  color: #b1b1b1;
}

.left-menu .nav li a {
  color: #626262;
  font-weight: normal;
}

.sub-li-title {
  cursor: pointer;
}

.sub-li a {
  background-color: #fff !important;
  color: #939393 !important;
}

.admin-tips li {
  margin-bottom: 0;
}

.nav-stacked > li {
  float: none;
}